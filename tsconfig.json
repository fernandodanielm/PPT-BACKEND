{
    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "resolveJsonModule": true
    },
    "include": [
      "index.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }